    "print(hard['combined'].dropna())\n",
    "print(grass_dirt['combined'].dropna())\n",
    "print(gravel['combined'].dropna())\n",
    "print(sand['combined'].dropna())\n",
    "print(astro_turf['combined'].dropna())\n",
    "# print(other.dropna())"
